package model;

import javax.annotation.ManagedBean;
import javax.faces.bean.RequestScoped;

@ManagedBean
@RequestScoped
public class ManagedBean1 {

	private String nombre="Hola mundo";
	public ManagedBean1() {
		
	}
	public String getNombre() {
		return nombre;
	}
	public void setNombre(String mensaje) {
		this.nombre = mensaje;
	}
}
