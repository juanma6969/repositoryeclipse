package model;

import java.io.Serializable;
import javax.persistence.*;
import java.util.Date;


/**
 * The persistent class for the tlineafactura database table.
 * 
 */
@Entity
@Table(name="tlineafactura")
@NamedQuery(name="LineaFactura.findAll", query="SELECT l FROM LineaFactura l")
public class LineaFactura implements Serializable {
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	private int idLineaFactura;

	private String descripcion;

	@Temporal(TemporalType.DATE)
	private Date fechaFactura;

	private byte pagado;

	public LineaFactura() {
	}

	public int getIdLineaFactura() {
		return this.idLineaFactura;
	}

	public void setIdLineaFactura(int idLineaFactura) {
		this.idLineaFactura = idLineaFactura;
	}

	public String getDescripcion() {
		return this.descripcion;
	}

	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}

	public Date getFechaFactura() {
		return this.fechaFactura;
	}

	public void setFechaFactura(Date fechaFactura) {
		this.fechaFactura = fechaFactura;
	}

	public byte getPagado() {
		return this.pagado;
	}

	public void setPagado(byte pagado) {
		this.pagado = pagado;
	}

}